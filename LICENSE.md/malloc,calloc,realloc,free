#include<stdio.h>
#include<stdlib.h>
int main()
{
    int a,i;
    printf("enter the nummbers:");
    scanf("%d",&a);
    int *b=(int*)malloc(a*sizeof(int));

    for(i=0;i<a;i++)
    {

      b[i]=i+1;
    }

    for(i=0;i<a;i++)
    {

        printf("%d",b[i]);
    }
}




